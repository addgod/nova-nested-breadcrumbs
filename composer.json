{
    "name": "addgod/nova-nested-breadcrumbs",
    "description": "Fully nested bradbcrumbs for laravel nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": ">=3"
    },
    "autoload": {
        "psr-4": {
            "Addgod\\NestedBreadcrumbs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Addgod\\NestedBreadcrumbs\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}